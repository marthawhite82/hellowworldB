pipeline {
  agent any
  stages {
    stage('Build'){
      steps {
        echo "build step"
        sleep 2
      }
    }
    stage('Test'){
      steps {
        echo "test step"
        sleep 2
      }
    }
     stage('Deploy'){
      steps {
        echo "build step"
        sleep 2
      }
    }
    stage('Docker image'){
      steps {
        echo "image step"
        sleep 2
      }
    }
    
  }
}
